    "assets": "src/assets/",
    "components": "src/components/",
    "containers": "src/containers/",
    "screens": "src/screens/",
    "common": "src/common/",
    "api": "src/api/",
    "route": "src/routes/",
    "actions": "src/actions/",
    "actionTypes": "src/actionTypes/",
    "reducer": "src/reducer/",
    "hoc": "src/hoc/",
    "selectors": "src/selectors/"
  },
  "eslintConfig": {
    "rules": {
      "no-unused-vars": 0,
      "@typescript-eslint/no-unused-vars": 0
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
